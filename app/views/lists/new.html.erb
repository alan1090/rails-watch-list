<div class="container d-flex justify-content-center">
  <div class="form-container" style="width: 400px;">
    <h1 class="my-5">Add a new list</h1>
    <%= simple_form_for @list do |f| %>
    <%= f.input :name %>
    <%= f.input :photo, as: :file %>
    <%= f.submit class: "btn btn-primary" %>
    <% end %>
  </div>
</div>
